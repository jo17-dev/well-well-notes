<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape
            android:shape="rectangle">
            <corners
                android:radius="8dp"/>
            <stroke
                android:width="1sp"
                android:color="?attr/colorPrimary"/>
            <padding
                android:left="8dp"
                android:right="8dp"/>

        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="8dp"/>
            <stroke
                android:width="1sp"
                android:color="@color/outlined_grey"/>
            <padding
                android:left="8dp"
                android:right="8dp"/>

        </shape>
    </item>

</selector>
